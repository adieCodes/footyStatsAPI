{
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "plugin:security/recommended"
  ],
  "plugins": ["prettier", "jest", "security"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prettier/prettier": "error",
    "jest/no-disabled-tests": "warn",
    "jest/no-identical-title": "error"
  },
  "env": {
    "jest/globals": true
  }
}
